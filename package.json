{
  "name": "admin-x",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:prod": "tsc --noEmit && vite build",
    "type:check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "eslint": "eslint \"**/*.{vue,js,ts,tsx}\"",
    "eslint:fix": "eslint \"**/*.{vue,js,ts,tsx}\" --fix",
    "stylelint": "stylelint \"**/*.{html,vue,css,less}\"",
    "stylelint:fix": "stylelint \"**/*.{html,vue,css,less}\" --fix",
    "prettier": "prettier --check \"**/*.{js,ts,json,tsx,css,scss,vue,html,md}\" --ignore-path .gitignore",
    "prettier:fix": "prettier --write  \"**/*.{js,ts,json,tsx,css,scss,vue,html,md}\" --ignore-path .gitignore",
    "extract-i18n": "node src/i18n/extractor.js",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run stylelint",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix && pnpm run stylelint:fix",
    "lint-staged": "npx lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "^13.4.0",
    "ant-design-vue": "4.2.6",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.1.0",
    "pinia": "^3.0.3",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.17",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.0",
    "@nabla/vite-plugin-eslint": "^2.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.15.33",
    "@types/sortablejs": "^1.15.8",
    "@unocss/vite": "^66.3.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^10.2.0",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss-less": "^6.0.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.21.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.35.0",
    "unocss": "66.1.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "6.3.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vue-eslint-parser": "^10.1.4",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0",
    "pnpm": ">=8.11.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,less,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  }
}
